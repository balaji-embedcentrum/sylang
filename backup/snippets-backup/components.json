{
  "Component Template": {
    "prefix": "component",
    "body": [
      "component ${1:ComponentName}",
      "  name \"${2:Component Display Name}\"",
      "  description \"${3:Component description}\"",
      "  owner \"${4:Team Name}\"",
      "  tags \"${5:tag1}\", \"${6:tag2}\"",
      "  safetylevel ${7|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}",
      "  aggregatedby ${8:ParentSubsystem}",
      "  interfaces",
      "    interface ${9:InterfaceName}",
      "      type ${10|Communication,Digital,Analog|}",
      "      protocol \"${11:protocol}\"",
      "      direction ${12|Input,Output,Bidirectional|}",
      "      $0",
      "  implements ${13:FunctionName}"
    ],
    "description": "Create a new component definition"
  },
  "Subsystem Template": {
    "prefix": "subsystem",
    "body": [
      "Subsystem ${1:SubsystemName}",
      "  name \"${2:Subsystem Display Name}\"",
      "  description \"${3:Subsystem description}\"",
      "  owner \"${4:Team Name}\"",
      "  tags \"${5:tag1}\", \"${6:tag2}\"",
      "  safetylevel ${7|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}",
      "  enables ${8:EnabledFunction}",
      "  implements ${9:FunctionName}",
      "  $0"
    ],
    "description": "Create a new subsystem definition"
  },
  "Interface Template": {
    "prefix": "interface",
    "body": [
      "interface ${1:InterfaceName}",
      "  type ${2|Communication,Digital,Analog|}",
      "  protocol \"${3:protocol}\"",
      "  direction ${4|Input,Output,Bidirectional|}",
      "  ${5:voltage \"${6:3.3V}\"}",
      "  $0"
    ],
    "description": "Create a new interface definition"
  },
  "Communication Interface": {
    "prefix": "comm_interface",
    "body": [
      "interface ${1:InterfaceName}",
      "  type Communication",
      "  protocol \"${2|SPI,I2C,CAN,LIN,UART|} ${3:speed}\"",
      "  direction ${4|Input,Output,Bidirectional|}",
      "  $0"
    ],
    "description": "Create a communication interface"
  },
  "Digital Interface": {
    "prefix": "digital_interface",
    "body": [
      "interface ${1:InterfaceName}",
      "  type Digital",
      "  voltage \"${2|3.3V,5V,12V,24V|} ${3|CMOS,TTL|}\"",
      "  direction ${4|Input,Output,Bidirectional|}",
      "  ${5:width \"${6:1-bit}\"}",
      "  $0"
    ],
    "description": "Create a digital interface"
  },
  "Requirements Section Template": {
    "prefix": "reqsection",
    "body": [
      "def reqsection ${1:ID}",
      "\tname \"${2:Name}\"",
      "\tdescription \"${3:Description}\"",
      "\t",
      "\tdef requirement ${4:FSR_ID}",
      "\t\tname \"${5:Name}\"",
      "\t\tdescription \"${6:Description}\"",
      "\t\ttype ${7|functionalsafety,functional,non-functional,performance,standards,legal,system,software,electronics,mechanics,test|}",
      "\t\tsource ${8|stakeholder,internal,supplier,customer|}",
      "\t\tderivedfrom ${9:safetygoal SG_ID}",
      "\t\tasil ${10|QM,A,B,C,D|}",
      "\t\trationale \"${11:Rationale}\"",
      "\t\tallocatedto ${12:subsystem Component1}, ${13:Component2}",
      "\t\tverificationcriteria \"${14:Criteria}\"",
      "\t\tstatus ${15|draft,review,approved|}",
      "\t\t$0"
    ],
    "description": "Create a complete requirements section"
  },
  "Requirement Template": {
    "prefix": "requirement",
    "body": [
      "def requirement ${1:FSR_ID}",
      "\tname \"${2:Name}\"",
      "\tdescription \"${3:Description}\"",
      "\ttype ${4|functionalsafety,functional,non-functional,performance,standards,legal,system,software,electronics,mechanics,test|}",
      "\tsource ${5|stakeholder,internal,supplier,customer|}",
      "\tderivedfrom ${6:safetygoal SG_ID}",
      "\tasil ${7|QM,A,B,C,D|}",
      "\trationale \"${8:Rationale}\"",
      "\tallocatedto ${9:subsystem Component1}, ${10:Component2}",
      "\tverificationcriteria \"${11:Criteria}\"",
      "\tstatus ${12|draft,review,approved|}",
      "\t$0"
    ],
    "description": "Create a single requirement definition"
  }
} 