{
  "SystemFunctions Container": {
    "prefix": "systemfunctions",
    "body": [
      "systemfunctions ${1:ContainerName}",
      "    def function ${2:FunctionName}",
      "        name \"${3:Display Name}\"",
      "        description \"${4:def function description}\"",
      "        owner \"${5:Team Name}\"",
      "        tags \"${6:tag1}\", \"${7:tag2}\"",
      "        safetylevel ${8|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}",
      "        enables ${9:FeatureName}"
    ],
    "description": "Complete system functions container with function"
  },
  "def function Definition": {
    "prefix": "function",
    "body": [
      "def function ${1:FunctionName}",
      "    name \"${2:Display Name}\"",
      "    description \"${3:def function description}\"",
      "    owner \"${4:Team Name}\"",
      "    tags \"${5:tag1}\", \"${6:tag2}\"",
      "    safetylevel ${7|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}",
      "    enables ${8:EnabledFeatures}"
    ],
    "description": "Complete def function definition"
  },
  "Basic Function": {
    "prefix": "func",
    "body": [
      "def function ${1:Name}",
      "    name \"${2:Display Name}\"",
      "    description \"${3:Description}\"",
      "    safetylevel ${4|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}"
    ],
    "description": "Basic def function structure"
  },
  "def function Name": {
    "prefix": "name",
    "body": [
      "name \"${1:Display Name}\""
    ],
    "description": "def function display name"
  },
  "def function Description": {
    "prefix": "desc",
    "body": [
      "description \"${1:def function description}\""
    ],
    "description": "def function description"
  },
  "def function Owner": {
    "prefix": "owner",
    "body": [
      "owner \"${1:Team Name}\""
    ],
    "description": "def function owner"
  },
  "def function Tags": {
    "prefix": "tags",
    "body": [
      "tags \"${1:tag1}\", \"${2:tag2}\", \"${3:tag3}\""
    ],
    "description": "def function tags"
  },
  "def function Enables": {
    "prefix": "enables",
    "body": [
      "enables ${1:Feature1}, ${2:Feature2}"
    ],
    "description": "Features enabled by this function"
  },
  "Safety Level": {
    "prefix": "safety",
    "body": [
      "safetylevel ${1|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}"
    ],
    "description": "def function safety level"
  }
} 