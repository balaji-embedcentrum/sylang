  "System Block": {
    "prefix": "system",
    "body": [
      "def block system ${1:SystemName}",
      "\tname \"${2:System Display Name}\"",
      "\tdescription \"${3:System description}\"",
      "\towner \"${4:System Team}\"",
      "\ttags \"${5:system}\", \"${6:tag2}\"",
      "\tasil ${7|QM,A,B,C,D|}",
      "\tcontains subsystem ${8:SubsystemName}",
      "\tpartof product ${9:ProductName}",
      "\t$0"
