{
  "ProductLine Definition": {
    "prefix": "productline",
    "body": [
      "productline ${1:ProductLineName}",
      "  name \"${2:ProductLineName}\"",
      "  description \"${3:A comprehensive description of the product line}\"",
      "  owner \"${4:Team Name}\", \"${5:Department}\"",
      "  domain \"${6:automotive}\", \"${7:safety}\"",
      "  compliance \"${8:ISO 26262}\", \"${9:ASPICE}\"",
      "  firstrelease \"${10:2025-01-01}\"",
      "  tags \"${11:tag1}\", \"${12:tag2}\", \"${13:tag3}\"",
      "  safetylevel ${14|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}",
      "  region \"${15:Global}\", \"${16:Europe}\", \"${17:North America}\""
    ],
    "description": "Complete product line definition template"
  },
  "Basic ProductLine": {
    "prefix": "ple",
    "body": [
      "productline ${1:Name}",
      "  name \"${2:Name}\"",
      "  description \"${3:Description}\"",
      "  owner \"${4:Team}\"",
      "  safetylevel ${5|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}"
    ],
    "description": "Basic product line structure"
  },
  "Description": {
    "prefix": "desc",
    "body": [
      "description \"${1:Enter description here}\""
    ],
    "description": "Description attribute"
  },
  "Owner": {
    "prefix": "owner",
    "body": [
      "owner \"${1:Team Name}\", \"${2:Department}\""
    ],
    "description": "Owner attribute with team and department"
  },
  "Safety Level": {
    "prefix": "safety",
    "body": [
      "safetylevel ${1|ASIL-A,ASIL-B,ASIL-C,ASIL-D,QM|}"
    ],
    "description": "Safety level definition"
  },
  "Tags": {
    "prefix": "tags",
    "body": [
      "tags \"${1:tag1}\", \"${2:tag2}\", \"${3:tag3}\""
    ],
    "description": "Tags for categorization"
  },
  "Compliance": {
    "prefix": "compliance",
    "body": [
      "compliance \"${1:ISO 26262}\", \"${2:ASPICE}\""
    ],
    "description": "Compliance standards"
  },
  "Domain": {
    "prefix": "domain",
    "body": [
      "domain \"${1:automotive}\", \"${2:safety}\""
    ],
    "description": "Application domain"
  },
  "Region": {
    "prefix": "region",
    "body": [
      "region \"${1:Global}\", \"${2:Europe}\", \"${3:North America}\""
    ],
    "description": "Geographic regions"
  }
} 