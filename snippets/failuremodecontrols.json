{
  "Control Measures": {
    "prefix": "controlmeasures",
    "body": [
      "def controlmeasures ${1:ControlMeasuresName}",
      "  ",
      "  // ========== PREVENTION MEASURES ==========",
      "  ",
      "  def prevention ${2:PreventionMeasureIdentifier}",
      "    name \"${3:Prevention Measure Name}\"",
      "    description \"${4:Description of prevention measure}\"",
      "    scope ${5:internal}",
      "    effectiveness ${6:high}",
      "    cost ${7:medium}",
      "    complexity ${8:moderate}",
      "    frequency ${9:continuous}",
      "    occurrencereduction ${10:80}",
      "    severityreduction ${11:60}",
      "    coverage ${12:complete}",
      "    independence ${13:high}",
      "    maturity ${14:mature}",
      "    implementation \"${15:Implementation details}\"",
      "    verification \"${16:Verification method}\"",
      "    responsibility \"${17:Team Name}\"",
      "    asil ${18:D}",
      "    depends measure ${19:DependentMeasure1}, ${20:DependentMeasure2}",
      "  ",
      "  // ========== DETECTION MEASURES ==========",
      "  ",
      "  def detection ${21:DetectionMeasureIdentifier}",
      "    name \"${22:Detection Measure Name}\"",
      "    description \"${23:Description of detection measure}\"",
      "    scope ${24:internal}",
      "    effectiveness ${25:high}",
      "    cost ${26:low}",
      "    complexity ${27:simple}",
      "    detecttime ${28:immediate}",
      "    detectionrating ${29:2}",
      "    coverage ${30:complete}",
      "    diagnosticcoverage ${31:95}",
      "    independence ${32:high}",
      "    maturity ${33:mature}",
      "    implementation \"${34:Implementation details}\"",
      "    verification \"${35:Verification method}\"",
      "    responsibility \"${36:Team Name}\"",
      "    asil ${37:D}",
      "  ",
      "  // ========== MITIGATION MEASURES ==========",
      "  ",
      "  def mitigation ${38:MitigationMeasureIdentifier}",
      "    name \"${39:Mitigation Measure Name}\"",
      "    description \"${40:Description of mitigation measure}\"",
      "    scope ${41:internal}",
      "    effectiveness ${42:high}",
      "    cost ${43:high}",
      "    complexity ${44:complex}",
      "    responsetime ${45:immediate}",
      "    severityreduction ${46:90}",
      "    coverage ${47:complete}",
      "    independence ${48:high}",
      "    maturity ${49:mature}",
      "    implementation \"${50:Implementation details}\"",
      "    verification \"${51:Verification method}\"",
      "    responsibility \"${52:Team Name}\"",
      "    asil ${53:D}",
      "    depends measure ${54:DependentMeasure1}, ${55:DependentMeasure2}",
      "  ",
      "  $0"
    ],
    "description": "Complete control measures template"
  },
