{
  "Fault Tree Analysis": {
    "prefix": "fta",
    "body": [
      "def faulttree ${1:SystemFaultTree}",
      "\tname \"${2:System Name - Fault Tree Analysis}\"",
      "\tdescription \"${3:Fault tree analysis for system safety-critical functions per ISO 26262 and IEC 61025}\"",
      "\towner \"${4:Safety Engineering Team}\"",
      "\treviewers \"${5:Systems Engineering}\", \"${6:Verification Team}\"",
      "\t",
      "\t# Standards compliance",
      "\tstandards \"ISO 26262\", \"IEC 61025\"",
      "\tanalysismethod \"${7:Deductive fault tree analysis}\"",
      "\t",
      "\t# Cross-references to existing safety files",
      "\titem ${8:SystemItem}",
      "\thazardidentification ${9:SystemHazards}",
      "\triskassessment ${10:SystemRiskAssessment}",
      "\tsafetygoals ${11:SystemSafetyGoals}",
      "\tproductline ${12:ProductLineName}",
      "\tsystemfeatures ${13:SystemFeatures}",
      "\tsystemfunctions ${14:SystemFunctions}",
      "\t",
      "\t# Top event definition",
      "\ttopevent",
      "\t\tdef topevent ${15:SystemHazardousEvent}",
      "\t\t\tname \"${16:System Hazardous Event}\"",
      "\t\t\tdescription \"${17:Description of the top-level hazardous event}\"",
      "\t\t\tseverity ${18:S3}",
      "\t\t\tasil ${19:D}",
      "\t\t\tprobability ${20:1.0e-6}",
      "\t",
      "\t# Intermediate events",
      "\tintermediateevents",
      "\t\tdef intermediateevent ${21:SubsystemFailure}",
      "\t\t\tname \"${22:Subsystem Failure}\"",
      "\t\t\tdescription \"${23:Intermediate event description}\"",
      "\t\t\tcategory ${24:systematic}",
      "\t\t\tprobability ${25:1.0e-5}",
      "\t",
      "\t# Basic events",
      "\tbasicevents",
      "\t\tdef basicevent ${26:ComponentFailure}",
      "\t\t\tname \"${27:Component Failure}\"",
      "\t\t\tdescription \"${28:Basic event description}\"",
      "\t\t\tcategory ${29:random}",
      "\t\t\tprobability ${30:1.0e-4}",
      "\t\t\tdormancy ${31:low}",
      "\t\t\texposuretime ${32:1.0}",
      "\t\t\trepairtime ${33:0.5}",
      "\t",
      "\t# Logic gates",
      "\tgates",
      "\t\tdef gate ${34:SystemLogicGate}",
      "\t\t\tname \"${35:System Logic Gate}\"",
      "\t\t\tdescription \"${36:Gate description}\"",
      "\t\t\tgatetype ${37:AND}",
      "\t\t\tinputs ${38:InputEvent1}, ${39:InputEvent2}",
      "\t\t\toutputs ${40:OutputEvent}",
      "\t",
      "\t# Transfer gates",
      "\ttransfers",
      "\t\tdef transfer ${41:SubsystemTransfer}",
      "\t\t\tname \"${42:Transfer to subsystem fault tree}\"",
      "\t\t\tdescription \"${43:Transfer description}\"",
      "\t\t\tfrom ${44:IntermediateEvent}",
      "\t\t\tto ${45:TransferEvent}",
      "\t\t\ttargetfta ${46:SubsystemFaultTree}",
      "$0"
    ],
    "description": "Complete fault tree analysis structure"
  },
  "Top Event": {
    "prefix": "topevent",
    "body": [
      "def topevent ${1:TopEventName}",
      "\tname \"${2:Top Event Name}\"",
      "\tdescription \"${3:Top event description}\"",
      "\tseverity ${4:S3}",
      "\tasil ${5:D}",
      "\tprobability ${6:1.0e-6}",
      "$0"
    ],
    "description": "Top event definition"
  },
  "Intermediate Event": {
    "prefix": "intermediateevent",
    "body": [
      "def intermediateevent ${1:IntermediateEventName}",
      "\tname \"${2:Intermediate Event Name}\"",
      "\tdescription \"${3:Intermediate event description}\"",
      "\tcategory ${4:systematic}",
      "\tprobability ${5:1.0e-5}",
      "$0"
    ],
    "description": "Intermediate event definition"
  },
  "Basic Event": {
    "prefix": "basicevent",
    "body": [
      "def basicevent ${1:BasicEventName}",
      "\tname \"${2:Basic Event Name}\"",
      "\tdescription \"${3:Basic event description}\"",
      "\tcategory ${4:random}",
      "\tprobability ${5:1.0e-4}",
      "\tdormancy ${6:low}",
      "\texposuretime ${7:1.0}",
      "\trepairtime ${8:0.5}",
      "$0"
    ],
    "description": "Basic event definition"
  },
  "AND Gate": {
    "prefix": "andgate",
    "body": [
      "def gate ${1:AndGateName}",
      "\tname \"${2:AND Gate Name}\"",
      "\tdescription \"${3:AND gate description}\"",
      "\tgatetype AND",
      "\tinputs ${4:InputEvent1}, ${5:InputEvent2}",
      "\toutputs ${6:OutputEvent}",
      "$0"
    ],
    "description": "AND logic gate"
  },
  "OR Gate": {
    "prefix": "orgate",
    "body": [
      "def gate ${1:OrGateName}",
      "\tname \"${2:OR Gate Name}\"",
      "\tdescription \"${3:OR gate description}\"",
      "\tgatetype OR",
      "\tinputs ${4:InputEvent1}, ${5:InputEvent2}",
      "\toutputs ${6:OutputEvent}",
      "$0"
    ],
    "description": "OR logic gate"
  },
  "NOT Gate": {
    "prefix": "notgate",
    "body": [
      "def gate ${1:NotGateName}",
      "\tname \"${2:NOT Gate Name}\"",
      "\tdescription \"${3:NOT gate description}\"",
      "\tgatetype NOT",
      "\tinputs ${4:InputEvent}",
      "\toutputs ${5:OutputEvent}",
      "$0"
    ],
    "description": "NOT logic gate"
  },
  "INHIBIT Gate": {
    "prefix": "inhibitgate",
    "body": [
      "def gate ${1:InhibitGateName}",
      "\tname \"${2:INHIBIT Gate Name}\"",
      "\tdescription \"${3:INHIBIT gate description}\"",
      "\tgatetype INHIBIT",
      "\tinputs ${4:PrimaryEvent}, ${5:InhibitCondition}",
      "\toutputs ${6:OutputEvent}",
      "\tcondition \"${7:Inhibit condition description}\"",
      "$0"
    ],
    "description": "INHIBIT logic gate"
  },
  "VOTING Gate": {
    "prefix": "votinggate",
    "body": [
      "def gate ${1:VotingGateName}",
      "\tname \"${2:VOTING Gate Name}\"",
      "\tdescription \"${3:K-out-of-N voting gate description}\"",
      "\tgatetype VOTING",
      "\tinputs ${4:InputEvent1}, ${5:InputEvent2}, ${6:InputEvent3}",
      "\toutputs ${7:OutputEvent}",
      "\tcondition \"${8:K-out-of-N condition}\"",
      "$0"
    ],
    "description": "VOTING logic gate (K-out-of-N)"
  },
  "Transfer Gate": {
    "prefix": "transfer",
    "body": [
      "def transfer ${1:TransferName}",
      "\tname \"${2:Transfer Name}\"",
      "\tdescription \"${3:Transfer to detailed fault tree}\"",
      "\tfrom ${4:SourceEvent}",
      "\tto ${5:TargetEvent}",
      "\ttargetfta ${6:TargetFaultTree}",
      "$0"
    ],
    "description": "Transfer gate to another fault tree"
  },
  "Property - Name": {
    "prefix": "name",
    "body": [
      "name \"${1:Name}\"",
      "$0"
    ],
    "description": "Name property"
  },
  "Property - Description": {
    "prefix": "description",
    "body": [
      "description \"${1:Description}\"",
      "$0"
    ],
    "description": "Description property"
  },
  "Property - Probability": {
    "prefix": "probability",
    "body": [
      "probability ${1:1.0e-6}",
      "$0"
    ],
    "description": "Probability property"
  },
  "Property - Severity": {
    "prefix": "severity",
    "body": [
      "severity ${1|S0,S1,S2,S3|}",
      "$0"
    ],
    "description": "Severity property"
  },
  "Property - ASIL": {
    "prefix": "asil",
    "body": [
      "asil ${1|QM,A,B,C,D|}",
      "$0"
    ],
    "description": "ASIL property"
  },
  "Property - Category": {
    "prefix": "category",
    "body": [
      "category ${1|systematic,random,external,common_cause,human_error|}",
      "$0"
    ],
    "description": "Category property"
  },
  "Property - Dormancy": {
    "prefix": "dormancy",
    "body": [
      "dormancy ${1|none,low,medium,high|}",
      "$0"
    ],
    "description": "Dormancy property"
  },
  "Property - Gate Type": {
    "prefix": "gatetype",
    "body": [
      "gatetype ${1|AND,OR,XOR,NAND,NOR,NOT,INHIBIT,PRIORITY_AND,VOTING|}",
      "$0"
    ],
    "description": "Gate type property"
  },
  "Property - Inputs": {
    "prefix": "inputs",
    "body": [
      "inputs ${1:InputEvent1}, ${2:InputEvent2}",
      "$0"
    ],
    "description": "Gate inputs property"
  },
  "Property - Outputs": {
    "prefix": "outputs",
    "body": [
      "outputs ${1:OutputEvent}",
      "$0"
    ],
    "description": "Gate outputs property"
  },
  "Property - Condition": {
    "prefix": "condition",
    "body": [
