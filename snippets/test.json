{
  "Complete Test Suite": {
    "prefix": "testsuite",
    "body": [
      "def testsuite ${1:TestSuiteName}",
      "  name \"${2:Test Suite Display Name}\"",
      "  description \"${3:Test suite description}\"",
      "  owner \"${4:Test Team}\"",
      "  tags \"${5:tag1}\", \"${6:tag2}\"",
      "  testtype ${7|unit,integration,system,acceptance,regression,smoke|}",
      "  asil ${8|A,B,C,D,QM|}",
      "  coverage ${9|statement,branch,mcdc,requirement,function|}",
      "  method ${10|manual,automated,hil,sil,mil,pil|}",
      "  ",
      "  def testcase ${11:TC_001}",
      "    name \"${12:Test Case Name}\"",
      "    description \"${13:Test case description}\"",
      "    priority ${14|critical,high,medium,low|}",
      "    asil ${15|A,B,C,D,QM|}",
      "    method ${16|manual,automated,hil,sil,mil,pil|}",
      "    verifies requirement ${17:RequirementList}",
      "    exercises ${18:FunctionList}",
      "    ",
      "    preconditions",
      "      \"${19:Precondition 1}\"",
      "      \"${20:Precondition 2}\"",
      "      ",
      "    teststeps",
      "      step ${21:STEP_001} \"${22:Step description}\"",
      "      step ${23:STEP_002} \"${24:Step description}\"",
      "      ",
      "    expectedresult \"${25:Expected result description}\"",
      "    testresult ${26|pass,fail,pending,inconclusive|}"
    ],
    "description": "Complete test suite with test case"
  },
